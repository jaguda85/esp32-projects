void wczytajUstawienia() {
  EEPROM.get(EEPROM_ADRES_USTAWIEN, ustawienia);
  if (ustawienia.gazMax > 4095 || ustawienia.sprzegloWcisnieteProc > 100 || ustawienia.sprzegloPuszczoneProc >= ustawienia.sprzegloWcisnieteProc || ustawienia.ledMaxTime > 15000 || ustawienia.ledMinTime >= ustawienia.ledMaxTime || (ustawienia.screenRotation != 1 && ustawienia.screenRotation != 3)) {
    ustawienia = Ustawienia();
    zapiszUstawienia();
  }
}

void zapiszUstawienia() {
  EEPROM.put(EEPROM_ADRES_USTAWIEN, ustawienia);
  EEPROM.commit();
}

void wyswietlMenuDiagnostyki() {
  if (wymusPelneRysowanie) {
    rysujTlo();
    tft.setTextSize(3); tft.setTextColor(ILI9341_YELLOW); tft.setCursor(50, 20); tft.print("DIAGNOSTYKA");
    tft.setTextSize(1); tft.setTextColor(ILI9341_CYAN); tft.setCursor(20, 225); tft.print("GORA/DOL: Wybor | OK: Akcja | SET: Powrot");
    
    String opcje[] = { "Podglad na zywo", "Ustawienia", "Odwracanie gazu: ", "Odwracanie sprzegla: ", "Testuj diode", "Powrot" };
    opcje[2] += (ustawienia.gazOdwracany ? "ON" : "OFF");
    opcje[3] += (ustawienia.sprzegloOdwracane ? "ON" : "OFF");

    tft.setTextSize(2);
    for(int i=0; i<6; i++){
      tft.setTextColor(i == pozycjaMenu ? ILI9341_CYAN : ILI9341_WHITE);
      tft.setCursor(20, 70 + i * 25);
      tft.print(opcje[i]);
    }
    wymusPelneRysowanie = false;
    wymusZaktualizowanieMenu = false;
  } else if (wymusZaktualizowanieMenu) {
    String opcje[] = { "Podglad na zywo", "Ustawienia", "Odwracanie gazu: ", "Odwracanie sprzegla: ", "Testuj diode", "Powrot" };
    opcje[2] += (ustawienia.gazOdwracany ? "ON" : "OFF");
    opcje[3] += (ustawienia.sprzegloOdwracane ? "ON" : "OFF");
    
    tft.setTextSize(2);
    tft.setTextColor(ILI9341_WHITE);
    tft.setCursor(20, 70 + poprzedniaPozycjaMenu * 25);
    tft.print(opcje[poprzedniaPozycjaMenu]);

    tft.setTextColor(ILI9341_CYAN);
    tft.setCursor(20, 70 + pozycjaMenu * 25);
    tft.print(opcje[pozycjaMenu]);
    
    wymusZaktualizowanieMenu = false;
  }
}

void obslugaMenuDiagnostyki(int kierunek) {
  poprzedniaPozycjaMenu = pozycjaMenu;
  pozycjaMenu = (pozycjaMenu + kierunek + 6) % 6;
  wymusZaktualizowanieMenu = true;
  wyswietlMenuDiagnostyki();
}

void wyswietlUstawieniaProgow() {
  if (wymusPelneRysowanie) {
    rysujTlo();
    tft.setTextSize(2); tft.setTextColor(ILI9341_YELLOW); tft.setCursor(80, 15); tft.print("USTAWIENIA");
    wymusPelneRysowanie = false;
  }

  String etykiety[] = {"Gaz MIN:", "Gaz MAX:", "Sprzeglo Puszczone:", "Sprzeglo Wcisniete:", "Czas LED MIN:", "Czas LED MAX:", "Orientacja:"};
  String strWartosci[7];
  for(int i=0; i<7; i++) {
    switch(i) {
      case 0: strWartosci[i] = String(ustawienia.gazMin); break;
      case 1: strWartosci[i] = String(ustawienia.gazMax); break;
      case 2: strWartosci[i] = String(ustawienia.sprzegloPuszczoneProc) + "%"; break;
      case 3: strWartosci[i] = String(ustawienia.sprzegloWcisnieteProc) + "%"; break;
      case 4: strWartosci[i] = String(ustawienia.ledMinTime); break;
      case 5: strWartosci[i] = String(ustawienia.ledMaxTime); break;
      case 6: strWartosci[i] = (ustawienia.screenRotation == 1) ? "Pozioma" : "Pozioma (Odw)"; break;
    }
  }

  if (wymusZaktualizowanieMenu && poprzedniaPozycjaMenu != pozycjaMenu) {
    tft.setTextSize(2);
    tft.setTextColor(ILI9341_WHITE);
    tft.setCursor(20, 50 + poprzedniaPozycjaMenu * 25);
    tft.print(etykiety[poprzedniaPozycjaMenu]);
    tft.fillRect(200, 50 + poprzedniaPozycjaMenu * 25, 110, 20, ILI9341_BLACK);
    tft.setCursor(200, 50 + poprzedniaPozycjaMenu * 25);
    tft.print(strWartosci[poprzedniaPozycjaMenu]);
    wymusZaktualizowanieMenu = false;
  }
  
  tft.setTextSize(2);
  for (int i=0; i < 7; i++) {
    bool jestEdytowany = (trybEdycjiUstawien && i == pozycjaMenu);
    tft.setTextColor(i == pozycjaMenu ? (jestEdytowany ? ILI9341_RED : ILI9341_CYAN) : ILI9341_WHITE);
    tft.setCursor(20, 50 + i * 25);
    tft.print(etykiety[i]);
    
    tft.fillRect(200, 50 + i * 25, 110, 20, ILI9341_BLACK);
    tft.setCursor(200, 50 + i * 25);
    tft.print(strWartosci[i]);
  }

  tft.setTextSize(1); tft.setTextColor(ILI9341_CYAN); 
  tft.fillRect(0, 220, tft.width(), 20, ILI9341_BLACK);
  if (trybEdycjiUstawien) {
    tft.setCursor(10, 225); tft.print("G/D: +/- 100 | L/P: +/- 10 | Przytrzymanie: szybciej");
  } else {
    tft.setCursor(10, 225); tft.print("G/D: Wybor | OK: Edytuj | SET: Powrot");
  }
}

void obslugaUstawienProgow(int zmiana) {
  if (!trybEdycjiUstawien) {
      poprzedniaPozycjaMenu = pozycjaMenu;
      pozycjaMenu = (pozycjaMenu + zmiana + 7) % 7;
      wymusZaktualizowanieMenu = true;
      wyswietlUstawieniaProgow();
      return;
  }
  
  long maxWartosc = 4095;
  switch(pozycjaMenu) {
    case 0: ustawienia.gazMin = constrain((long)ustawienia.gazMin + zmiana, 0, maxWartosc); break;
    case 1: ustawienia.gazMax = constrain((long)ustawienia.gazMax + zmiana, 0, maxWartosc); break;
    case 2: ustawienia.sprzegloPuszczoneProc = constrain((long)ustawienia.sprzegloPuszczoneProc + (zmiana/10), 0, 100); break;
    case 3: ustawienia.sprzegloWcisnieteProc = constrain((long)ustawienia.sprzegloWcisnieteProc + (zmiana/10), 0, 100); break;
    case 4: ustawienia.ledMinTime = constrain((long)ustawienia.ledMinTime + zmiana, 0, 15000); break;
    case 5: ustawienia.ledMaxTime = constrain((long)ustawienia.ledMaxTime + zmiana, 0, 15000); break;
    case 6: ustawienia.screenRotation = (ustawienia.screenRotation == 1) ? 3 : 1; tft.setRotation(ustawienia.screenRotation); break;
  }
  wyswietlUstawieniaProgow();
}
