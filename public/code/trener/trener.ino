void setup() {
  Serial.begin(115200);
  tft.begin();
  EEPROM.begin(2048);
  
  wczytajUstawienia();
  tft.setRotation(ustawienia.screenRotation);

  pinMode(LED_ZIELONY, OUTPUT);
  pinMode(PRZYCISK_GORA, INPUT_PULLUP);
  pinMode(PRZYCISK_DOL, INPUT_PULLUP);
  pinMode(PRZYCISK_LEWO, INPUT_PULLUP);
  pinMode(PRZYCISK_PRAWO, INPUT_PULLUP);
  pinMode(PRZYCISK_OK, INPUT_PULLUP);
  pinMode(PRZYCISK_SET, INPUT_PULLUP);
  pinMode(PRZYCISK_START, INPUT_PULLUP);

  digitalWrite(LED_ZIELONY, LOW);
  
  sdDostepna = inicjujKarteSD();
  if (sdDostepna) {
    wczytajZawodnikow();
    odczytajNajlepszyWynik();
  }
  
  wyswietlLogoStartowe();
  randomSeed(analogRead(39));
  rysujEkranPowitalny();
}

void loop() {
  unsigned long teraz = millis();
  bool stanGora = digitalRead(PRZYCISK_GORA);
  bool stanDol = digitalRead(PRZYCISK_DOL);
  bool stanLewo = digitalRead(PRZYCISK_LEWO);
  bool stanPrawo = digitalRead(PRZYCISK_PRAWO);
  bool stanOK = digitalRead(PRZYCISK_OK);
  bool stanSet = digitalRead(PRZYCISK_SET);
  bool stanStart = digitalRead(PRZYCISK_START);

  if (teraz - czasOstatniegoNacisniecia > OPOZNIENIE_PRZYCISKOW) {
    // START
    if (stanStart == LOW && poprzedniStanStart == HIGH) {
      czasOstatniegoNacisniecia = teraz;
      if (tryb == TRYB_POWITALNY) {
        tryb = TRYB_TEST;
        stanTestu = 0; 
        wymusPelneRysowanie = true;
      } else if (tryb == TRYB_TEST && (stanTestu == 3 || stanTestu == 4)) {
        tryb = TRYB_POWITALNY;
        rysujEkranPowitalny();
      } else if (tryb == TRYB_DODAJ_ZAWODNIKA) {
        if (pozycjaKursora < MAX_DLUGOSC_NICKU) {
          if (nowyNick.length()<=pozycjaKursora) nowyNick+=wybranyZnak; else nowyNick.setCharAt(pozycjaKursora, wybranyZnak);
        }
        nowyNick.trim(); 
        if(nowyNick.length()>0) { zapiszNowegoZawodnika(nowyNick); aktualnyZawodnik=nowyNick; }
        tryb=TRYB_POWITALNY;
        rysujEkranPowitalny();
      }
    }
    // SET -> COFNIJ
    if (stanSet == LOW && poprzedniStanSet == HIGH) {
      czasOstatniegoNacisniecia = teraz;
      wymusPelneRysowanie = true;
      trybEdycjiUstawien = false;
      switch(tryb) {
        case TRYB_POWITALNY: tryb = TRYB_DIAGNOSTYKA_MENU; pozycjaMenu = 0; wyswietlMenuDiagnostyki(); break;
        case TRYB_TEST: tryb = TRYB_POWITALNY; digitalWrite(LED_ZIELONY, LOW); rysujEkranPowitalny(); break;
        case TRYB_MENU_ZAWODNIKA:
        case TRYB_REKORDY:
        case TRYB_DIAGNOSTYKA_MENU: tryb = TRYB_POWITALNY; rysujEkranPowitalny(); break;
        case TRYB_WYBOR_ZAWODNIKA:
        case TRYB_DODAJ_ZAWODNIKA:
        case TRYB_USUN_ZAWODNIKA: tryb = TRYB_MENU_ZAWODNIKA; pozycjaMenu = 0; wyswietlMenuZawodnika(); break;
        case TRYB_DIAGNOSTYKA_LIVE:
        case TRYB_DIAGNOSTYKA_PROGI: tryb = TRYB_DIAGNOSTYKA_MENU; pozycjaMenu = 0; wyswietlMenuDiagnostyki(); break;
        case TRYB_REKORDY_POTWIERDZENIE_USUNIECIA: tryb = TRYB_REKORDY; wyswietlEkranRekordow(); break;
        case TRYB_USUN_ZAWODNIKA_POTWIERDZENIE: tryb = TRYB_USUN_ZAWODNIKA; wyswietlListeDoUsuniecia(); break;
        case TRYB_USUN_WYNIKI_GOSCIA_POTWIERDZENIE: tryb = TRYB_REKORDY; wyswietlEkranRekordow(); break;
      }
    }
    // OK
    if (stanOK == LOW && poprzedniStanOK == HIGH) {
      czasOstatniegoNacisniecia = teraz;
      if (tryb == TRYB_POWITALNY) {
        wymusPelneRysowanie = true; tryb = TRYB_MENU_ZAWODNIKA; pozycjaMenu = 0; wyswietlMenuZawodnika();
      } else {
        obslugaMenuOK();
      }
    }
    // NAWIGACJA
    if (stanGora == LOW && poprzedniStanGora == HIGH) {
      czasOstatniegoNacisniecia = teraz;
      if (tryb == TRYB_REKORDY) { obslugaPrzewijaniaRekordow(-1); }
      else if (tryb == TRYB_DIAGNOSTYKA_MENU) { obslugaMenuDiagnostyki(-1); }
      else if (tryb == TRYB_DIAGNOSTYKA_PROGI) { obslugaUstawienProgow(trybEdycjiUstawien ? 100 : -1); }
      else { obslugaMenuNawigacja(-1); }
    }
    if (stanDol == LOW && poprzedniStanDol == HIGH) {
      czasOstatniegoNacisniecia = teraz;
      if(tryb == TRYB_REKORDY) { obslugaPrzewijaniaRekordow(1); }
      else if (tryb == TRYB_DIAGNOSTYKA_MENU) { obslugaMenuDiagnostyki(1); }
      else if (tryb == TRYB_DIAGNOSTYKA_PROGI) { obslugaUstawienProgow(trybEdycjiUstawien ? -100 : 1); }
      else { obslugaMenuNawigacja(1); }
    }
    if (stanLewo == LOW && poprzedniStanLewo == HIGH) {
      czasOstatniegoNacisniecia = teraz;
      if (tryb == TRYB_POWITALNY) { tryb = TRYB_REKORDY; pozycjaPrzewijania = 0; wymusPelneRysowanie = true; wyswietlEkranRekordow(); } 
      else if (tryb == TRYB_DIAGNOSTYKA_PROGI) { if(trybEdycjiUstawien) obslugaUstawienProgow(-10); }
      else { obslugaMenuNawigacjaPozioma(-1); }
    }
    if (stanPrawo == LOW && poprzedniStanPrawo == HIGH) {
      czasOstatniegoNacisniecia = teraz;
      if (tryb == TRYB_DIAGNOSTYKA_PROGI) { if(trybEdycjiUstawien) obslugaUstawienProgow(10); }
      else { obslugaMenuNawigacjaPozioma(1); }
    }
  }

  if (tryb == TRYB_DODAJ_ZAWODNIKA) {
    if ((stanLewo == LOW || stanPrawo == LOW) && (teraz - czasOstatniegoNacisniecia > OPOZNIENIE_PRZED_PRZEWIJANIEM)) {
      if (teraz - czasOstatniegoNacisniecia > SZYBKOSC_PRZEWIJANIA_ZNAKOW) {
        if (stanLewo == LOW) obslugaMenuNawigacjaPozioma(-1); else if (stanPrawo == LOW) obslugaMenuNawigacjaPozioma(1);
        czasOstatniegoNacisniecia = teraz;
      }
    }
  }
  
  poprzedniStanGora = stanGora; poprzedniStanDol = stanDol; poprzedniStanLewo = stanLewo;
  poprzedniStanPrawo = stanPrawo; poprzedniStanOK = stanOK; poprzedniStanSet = stanSet;
  poprzedniStanStart = stanStart;
  
  if (tryb == TRYB_TEST) { obslugaTestu(); } 
  else if (tryb == TRYB_DIAGNOSTYKA_LIVE) {
    if(wymusPelneRysowanie) {
      wyswietlEkranDiagnostykiLive();
      wymusPelneRysowanie = false;
    }
    wartoscGazu = analogRead(POT_GAZ);
    int procentGazu = map(wartoscGazu, ustawienia.gazMin, ustawienia.gazMax, ustawienia.gazOdwracany ? 100 : 0, ustawienia.gazOdwracany ? 0 : 100);
    procentGazu = constrain(procentGazu, 0, 100);
    
    int wartoscSprzeglaADC = analogRead(POT_SPRZEGLO);
    int procentSprzegla = map(wartoscSprzeglaADC, 0, 4095, ustawienia.sprzegloOdwracane ? 100 : 0, ustawienia.sprzegloOdwracane ? 0 : 100);
    procentSprzegla = constrain(procentSprzegla, 0, 100);
    
    aktualizujEkranDiagnostyki(procentGazu, procentSprzegla);
  } else if (tryb == TRYB_POWITALNY) {
    obslugaMruganiaNicku();
  }
  delay(5);
}
