void usunWynikiGoscia() {
  if (!sdDostepna) return;
  
  File plik = SD.open(nazwaPlikuWynikow, FILE_READ);
  if (!plik) return;
  
  String trescPliku = "Nr,Data,Czas_ms,Gaz_%,Status,Zawodnik\n";
  String trescTymczasowa = "";
  plik.readStringUntil('\n'); // Pomijamy nagłówek
  
  while (plik.available()) {
    String linia = plik.readStringUntil('\n');
    if (linia.length() > 0) {
      int p5 = linia.lastIndexOf(',');
      if (p5 > 0) {
        String nick = linia.substring(p5+1);
        nick.trim();
        // Usuwamy końce linii jeśli są obecne
        if (nick.endsWith("\r")) nick.remove(nick.length()-1);
        if (nick.endsWith("\n")) nick.remove(nick.length()-1);
        // Zachowujemy tylko wyniki innych niż GOŚĆ
        if (nick != "GOSC" && nick != "GOSC\r" && nick != "GOSC\n") {
          trescTymczasowa += linia;
        }
      }
    }
  }
  plik.close();
  
  SD.remove(nazwaPlikuWynikow);
  File nowyPlik = SD.open(nazwaPlikuWynikow, FILE_WRITE);
  if (nowyPlik) {
    nowyPlik.print(trescPliku);
    nowyPlik.print(trescTymczasowa);
    nowyPlik.close();
  }
  
  numerTestu = 0;
  najlepszyWynik = 9999;
  odczytajNajlepszyWynik();
  liczbaWynikow = 0; // Resetujemy liczbę wyników
  
  tft.fillRect(50,100,220,40,ILI9341_BLACK); tft.setTextSize(2); tft.setTextColor(ILI9341_RED);
  tft.setCursor(60, 110); tft.print("Wyniki Goscia usuniete!"); delay(1500);
  
  // Odśwież wyświetlanie rekordów
  odczytajPosortujIWyswietlRekordy();
}

void obslugaPrzewijaniaRekordow(int kierunek) {
  bool p = false;
  if(kierunek > 0 && pozycjaPrzewijania < liczbaWynikow-WIERSZY_NA_EKRANIE){
    pozycjaPrzewijania++;
    p = true;
  }
  if(kierunek < 0 && pozycjaPrzewijania > 0){
    pozycjaPrzewijania--;
    p = true;
  }
  if(p){
    odczytajPosortujIWyswietlRekordy();
  }
}