void rysujTlo() {
  uint16_t kolorTla, kolorWzoru;

  // Ustawiamy ciemne tło dla wszystkich ekranów dla lepszej czytelności
  kolorTla = ILI9341_BLACK;

  // Wybieramy kolor wzoru bitmapy w zależności od trybu, aby nadać mu charakter
  // Wzór jest teraz subtelny i ciemny, aby nie rozpraszać.
  switch (tryb) {
    case TRYB_POWITALNY:
      // Stonowany, niebieskawy wzór
      kolorWzoru = ILI9341_NAVY;
      break;

    case TRYB_TEST:
      // Czerwony, "alarmowy" wzór
      kolorWzoru = ILI9341_MAROON;
      break;

    case TRYB_REKORDY:
    case TRYB_MENU_ZAWODNIKA:
    case TRYB_WYBOR_ZAWODNIKA:
    case TRYB_DODAJ_ZAWODNIKA:
    case TRYB_USUN_ZAWODNIKA:
    case TRYB_REKORDY_POTWIERDZENIE_USUNIECIA:
    case TRYB_USUN_ZAWODNIKA_POTWIERDZENIE:
    case TRYB_USUN_WYNIKI_GOSCIA:
    case TRYB_USUN_WYNIKI_GOSCIA_POTWIERDZENIE:
      // Bardzo subtelny, szary wzór dla menu
      kolorWzoru = ILI9341_DARKGREY;
      break;
    
    case TRYB_DIAGNOSTYKA_MENU:
    case TRYB_DIAGNOSTYKA_LIVE:
    case TRYB_DIAGNOSTYKA_PROGI:
      // Zielony, "techniczny" wzór
      kolorWzoru = ILI9341_DARKGREEN;
      break;

    default: // Domyślny schemat
      kolorWzoru = ILI9341_DARKGREY;
      break;
  }

  // Najpierw wypełnij ekran jednolitym, ciemnym kolorem
  tft.fillScreen(kolorTla);
  // Następnie narysuj na nim bitmapę w wybranym, ciemnym kolorze
  tft.drawBitmap(0, 0, epd_bitmap_zuzelSmile, 320, 240, kolorWzoru);
}
