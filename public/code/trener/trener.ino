void obslugaMenuNawigacja(int kierunek) {
  poprzedniaPozycjaMenu = pozycjaMenu; // ZAPAMIĘTAJ POPRZEDNIĄ POZYCJĘ
  
  if (tryb == TRYB_MENU_ZAWODNIKA) {
    pozycjaMenu = (pozycjaMenu + kierunek + 5) % 5;
    wymusZaktualizowanieMenu = true; // UŻYJ NOWEJ FLAGI
    wyswietlMenuZawodnika();
  } else if (tryb == TRYB_WYBOR_ZAWODNIKA || tryb == TRYB_USUN_ZAWODNIKA) {
    int limit = (tryb == TRYB_WYBOR_ZAWODNIKA) ? liczbaZawodnikow + 1 : liczbaZawodnikow;
    if (limit == 0) return;
    
    int staraPozycjaPrzewijania = pozycjaPrzewijania;
    pozycjaMenu = (pozycjaMenu + kierunek + limit) % limit;

    if (pozycjaMenu < pozycjaPrzewijania) pozycjaPrzewijania = pozycjaMenu;
    if (pozycjaMenu >= pozycjaPrzewijania + WIERSZY_LISTY_ZAWODNIKOW) pozycjaPrzewijania = pozycjaMenu - WIERSZY_LISTY_ZAWODNIKOW + 1;

    if (staraPozycjaPrzewijania == pozycjaPrzewijania) {
        wymusZaktualizowanieMenu = true; // Tylko zmiana kursora, bez przewijania
    } else {
        wymusPelneRysowanie = true; // Było przewijanie, trzeba odrysować całą listę
    }
    
    if(tryb == TRYB_WYBOR_ZAWODNIKA) wyswietlListeZawodnikow(); else wyswietlListeDoUsuniecia();
  }
}

void obslugaMenuNawigacjaPozioma(int kierunek) {
  if (tryb == TRYB_DODAJ_ZAWODNIKA) {
    if (pozycjaKursora < MAX_DLUGOSC_NICKU) {
      indeksZnaku=(indeksZnaku+kierunek+LICZBA_ZNAKOW)%LICZBA_ZNAKOW; wybranyZnak=DOSTEPNE_ZNAKI[indeksZnaku];
    } else { wybranyZnak = (wybranyZnak == 'Z') ? 'P' : 'Z'; }
    wyswietlDodawanieZawodnika(nowyNick, pozycjaKursora, wybranyZnak);
  } else if (tryb == TRYB_REKORDY_POTWIERDZENIE_USUNIECIA) {
      pozycjaMenuPotwierdzenia = (kierunek > 0) ? 0 : 1;
      wyswietlPotwierdzenieUsuniecia();
  } else if (tryb == TRYB_USUN_ZAWODNIKA_POTWIERDZENIE) {
      pozycjaMenuPotwierdzenia = (kierunek > 0) ? 1 : 0;
      wyswietlPotwierdzenieUsunieciaZawodnika();
  } else if (tryb == TRYB_USUN_WYNIKI_GOSCIA_POTWIERDZENIE) {
      pozycjaMenuPotwierdzenia = (kierunek > 0) ? 1 : 0;
      wyswietlPotwierdzenieUsunieciaGoscia();
  }
}

void obslugaTestu() {
    wartoscGazu = analogRead(POT_GAZ);
    int pG = map(wartoscGazu, ustawienia.gazMin, ustawienia.gazMax, ustawienia.gazOdwracany ? 100 : 0, ustawienia.gazOdwracany ? 0 : 100);
    pG = constrain(pG, 0, 100);

    int wartoscSprzeglaADC = analogRead(POT_SPRZEGLO);
    int pS = map(wartoscSprzeglaADC, 0, 4095, ustawienia.sprzegloOdwracane ? 100 : 0, ustawienia.sprzegloOdwracane ? 0 : 100);
    pS = constrain(pS, 0, 100);
    
    unsigned long teraz = millis();

    switch(stanTestu){
        case 0: // Czekanie na wciśnięcie sprzęgła
            if (wymusPelneRysowanie) {
              rysujTlo();
              tft.setTextSize(3); tft.setTextColor(ILI9341_YELLOW); 
              tft.setCursor(15, 80); tft.print("WCISNIJ SPRZEGLO");
              tft.setCursor(45, 120); tft.print("I PRZYTRZYMAJ");
              wymusPelneRysowanie = false;
            }
            if (pS >= ustawienia.sprzegloWcisnieteProc) {
                digitalWrite(LED_ZIELONY, HIGH);
                stanTestu = 1; 
                rysujSuwakiTestu(pG, pS);
                wyswietlInfoTestu("TRZYMAJ SPRZEGLO");
                czasStartu = teraz;
                czasFazy = random(ustawienia.ledMinTime, ustawienia.ledMaxTime);
            }
            break;

        case 1: // Oczekiwanie na sygnał do puszczenia sprzęgła
            aktualizujSuwakiTestu(pG, pS);
            if (pS < ustawienia.sprzegloWcisnieteProc - 5){
                stanTestu = 4;
                digitalWrite(LED_ZIELONY, LOW); 
                rysujTlo();
                tft.setTextSize(3);tft.setTextColor(ILI9341_RED);
                tft.setCursor(100, 80);tft.print("TASMA!"); 
                tft.setTextSize(2);tft.setCursor(20, 140);tft.print("Pusciles za wczesnie!");
                zapiszWynik(0, pG, "FALSZART");
            } else if (teraz - czasStartu >= czasFazy) {
                stanTestu = 2;
                digitalWrite(LED_ZIELONY, LOW); 
                rysujSuwakiTestu(pG, pS);
                wyswietlInfoTestu("PUSC SPRZEGLO!!!");
                czasStartu = teraz;
            }
            break;

        case 2: // Oczekiwanie na puszczenie sprzęgła
            aktualizujSuwakiTestu(pG, pS);
            if (pS <= ustawienia.sprzegloPuszczoneProc) {
                czasReakcji = teraz - czasStartu;
                procentGazuWyniku = pG;
                stanTestu = 3; 
                rysujTlo();
                tft.setTextSize(3);tft.setTextColor(ILI9341_GREEN);tft.setCursor(120,20);tft.print("WYNIK");
                tft.setTextSize(2);tft.setTextColor(ILI9341_MAGENTA);tft.setCursor(60,55);tft.print("Zawodnik: ");tft.print(aktualnyZawodnik);
                tft.setTextSize(3);tft.setTextColor(ILI9341_WHITE);tft.setCursor(95,85);tft.print(czasReakcji);tft.print(" ms");
                tft.drawLine(0, 110, 320, 110, ILI9341_RED);
                tft.setTextSize(2);tft.setTextColor(ILI9341_CYAN);tft.setCursor(40,120);tft.print("Najlepszy: ");tft.print(najlepszyWynik);tft.print(" ms");
                tft.setTextSize(1);tft.setCursor(60, 140); tft.print("("); tft.print(najlepszyZawodnik); tft.print(")");
                tft.drawFastHLine(0, 155, 320, ILI9341_DARKGREEN);
                
                if (procentGazuWyniku < 75) {
                    tft.setTextSize(2);tft.setTextColor(ILI9341_RED);tft.setCursor(30,165);tft.print("Malo gazu: ");tft.print(procentGazuWyniku);tft.print("%");
                    zapiszWynik(czasReakcji, procentGazuWyniku, "OK_MALO_GAZU");
                } else {
                    tft.setTextSize(2);tft.setTextColor(ILI9341_YELLOW);tft.setCursor(50,175);tft.print("Poziom gazu: ");tft.print(procentGazuWyniku);tft.print("%");
                    zapiszWynik(czasReakcji, procentGazuWyniku, "OK");
                }
            } else if (teraz - czasStartu > 2000) {
                stanTestu = 4;
                rysujTlo();
                tft.setTextSize(3);tft.setTextColor(ILI9341_RED);tft.setCursor(40,100);tft.print("BRAK REAKCJI!");
                zapiszWynik(2000, pG, "BRAK_REAKCJI");
            }
            break;
    }
    if (stanTestu == 3 || stanTestu == 4){
        tft.setTextSize(1);tft.setTextColor(ILI9341_WHITE);
        tft.setCursor(60, 230); tft.print("START/SET aby powrocic");
    }
}

void obslugaMenuOK() {
  wymusPelneRysowanie = true;
  if (tryb == TRYB_MENU_ZAWODNIKA) {
    switch (pozycjaMenu) {
      case 0: tryb = TRYB_WYBOR_ZAWODNIKA; pozycjaPrzewijania = 0; break;
      case 1: tryb = TRYB_DODAJ_ZAWODNIKA; pozycjaKursora=0; indeksZnaku=0; wybranyZnak=' '; nowyNick=""; break;
      case 2: tryb = TRYB_USUN_ZAWODNIKA; pozycjaPrzewijania = 0; break;
      case 3: aktualnyZawodnik = "GOSC"; tryb = TRYB_POWITALNY; break;
      case 4: tryb = TRYB_POWITALNY; break;
    }
  } else if (tryb == TRYB_WYBOR_ZAWODNIKA) {
    aktualnyZawodnik = (pozycjaMenu == 0) ? "GOSC" : listaZawodnikow[pozycjaMenu-1];
    tryb = TRYB_POWITALNY;
  } else if (tryb == TRYB_USUN_ZAWODNIKA) {
    if (liczbaZawodnikow > 0) {
      indeksZawodnikaDoUsuniecia = pozycjaMenu; tryb = TRYB_USUN_ZAWODNIKA_POTWIERDZENIE;
      pozycjaMenuPotwierdzenia = 1;
    }
  } else if (tryb == TRYB_USUN_ZAWODNIKA_POTWIERDZENIE) {
    if (pozycjaMenuPotwierdzenia == 0) { usunZawodnika(indeksZawodnikaDoUsuniecia); pozycjaMenu=0; pozycjaPrzewijania = 0;}
    tryb = TRYB_USUN_ZAWODNIKA;
  } else if (tryb == TRYB_DODAJ_ZAWODNIKA) {
    if (pozycjaKursora < MAX_DLUGOSC_NICKU) {
      if (nowyNick.length()<=pozycjaKursora) nowyNick+=wybranyZnak; else nowyNick.setCharAt(pozycjaKursora, wybranyZnak);
      pozycjaKursora++;
      if (pozycjaKursora>=MAX_DLUGOSC_NICKU) wybranyZnak='Z'; else { indeksZnaku=0; wybranyZnak=DOSTEPNE_ZNAKI[indeksZnaku]; }
    } else {
      if (wybranyZnak == 'Z') {
        nowyNick.trim(); 
        if(nowyNick.length()>0) { zapiszNowegoZawodnika(nowyNick); aktualnyZawodnik=nowyNick; }
        tryb=TRYB_POWITALNY;
        rysujEkranPowitalny();
      } else {
        tryb=TRYB_MENU_ZAWODNIKA;
      }
    }
  } else if (tryb == TRYB_DIAGNOSTYKA_MENU) {
    switch(pozycjaMenu) {
        case 0: tryb = TRYB_DIAGNOSTYKA_LIVE; break;
        case 1: tryb = TRYB_DIAGNOSTYKA_PROGI; trybEdycjiUstawien = false; break;
        case 2: ustawienia.gazOdwracany = !ustawienia.gazOdwracany; zapiszUstawienia(); wymusPelneRysowanie = false; wyswietlMenuDiagnostyki(); return;
        case 3: ustawienia.sprzegloOdwracane = !ustawienia.sprzegloOdwracane; zapiszUstawienia(); wymusPelneRysowanie = false; wyswietlMenuDiagnostyki(); return;
        case 4: digitalWrite(LED_ZIELONY, HIGH); delay(2000); digitalWrite(LED_ZIELONY, LOW); return;
        case 5: tryb = TRYB_POWITALNY; break;
    }
  } else if (tryb == TRYB_DIAGNOSTYKA_PROGI) {
    trybEdycjiUstawien = !trybEdycjiUstawien;
    if (!trybEdycjiUstawien) zapiszUstawienia();
  } else if(tryb == TRYB_REKORDY) {
    tryb = TRYB_REKORDY_POTWIERDZENIE_USUNIECIA;
    pozycjaMenuPotwierdzenia = 1;
  } else if(tryb == TRYB_REKORDY_POTWIERDZENIE_USUNIECIA) {
    if(pozycjaMenuPotwierdzenia == 0) {
        usunWszystkieRekordy();
    }
    tryb = TRYB_REKORDY;
  } else if (tryb == TRYB_USUN_WYNIKI_GOSCIA) {
    tryb = TRYB_USUN_WYNIKI_GOSCIA_POTWIERDZENIE;
    pozycjaMenuPotwierdzenia = 1;
  } else if (tryb == TRYB_USUN_WYNIKI_GOSCIA_POTWIERDZENIE) {
    if(pozycjaMenuPotwierdzenia == 0) {
        usunWynikiGoscia();
    }
    tryb = TRYB_REKORDY;
  }
  
  switch(tryb) {
      case TRYB_POWITALNY: rysujEkranPowitalny(); break;
      case TRYB_MENU_ZAWODNIKA: wyswietlMenuZawodnika(); break;
      case TRYB_WYBOR_ZAWODNIKA: wyswietlListeZawodnikow(); break;
      case TRYB_DODAJ_ZAWODNIKA: wyswietlDodawanieZawodnika(nowyNick, pozycjaKursora, wybranyZnak); break;
      case TRYB_USUN_ZAWODNIKA: wyswietlListeDoUsuniecia(); break;
      case TRYB_USUN_ZAWODNIKA_POTWIERDZENIE: wyswietlPotwierdzenieUsunieciaZawodnika(); break;
      case TRYB_DIAGNOSTYKA_LIVE: wyswietlEkranDiagnostykiLive(); break;
      case TRYB_DIAGNOSTYKA_PROGI: wyswietlUstawieniaProgow(); break;
      case TRYB_REKORDY: wyswietlEkranRekordow(); break;
      case TRYB_REKORDY_POTWIERDZENIE_USUNIECIA: wyswietlPotwierdzenieUsuniecia(); break;
      case TRYB_USUN_WYNIKI_GOSCIA: wyswietlEkranRekordow(); break;
      case TRYB_USUN_WYNIKI_GOSCIA_POTWIERDZENIE: wyswietlPotwierdzenieUsunieciaGoscia(); break;
  }
}
